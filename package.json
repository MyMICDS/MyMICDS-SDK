{
  "name": "@mymicds/sdk",
  "version": "1.3.0",
  "description": "Client for connecting to the MyMICDS API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc --out ./docs --mode file ./src",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run lint && npm run ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts": "npm run clean && tsc",
    "ts:watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyMICDS/MyMICDS-SDK.git"
  },
  "keywords": [
    "mymicds",
    "micds"
  ],
  "author": "MyMICDS Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyMICDS/MyMICDS-SDK/issues"
  },
  "homepage": "https://github.com/MyMICDS/MyMICDS-SDK#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.20.1",
    "qs": "^6.5.1",
    "rxjs": "^6.3.3"
  },
  "peerDependencies": {
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "@mymicds/configs": "^1.0.0",
    "@types/form-data": "^2.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^8.5.2",
    "@types/qs": "^6.5.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.1",
    "typedoc": "^0.9.0",
    "typescript": "~3.1.1"
  },
  "_moduleAliases": {
    "@sdk": "dist",
    "@libs": "dist/libs"
